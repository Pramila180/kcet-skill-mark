{
  "version": 2,
  "builds": [
    {
      "src": "api/simple_app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/simple_app.py"
    }
  ],
  "env": {
    "SECRET_KEY": "@secret_key"
  }
}